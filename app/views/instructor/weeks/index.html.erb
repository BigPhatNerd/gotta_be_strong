<br />
<br />
<div class="row ">
    <div class="booyah-box col-10 offset-1 ">
        <h1 class="text-center">Instructor Weeks Index Page</h1>
        <% @program_names.each do |program_name| %>
        <h1>
            <u>
                <%=  program_name.program_name%></u>
        </h1>
        <% @weeks.each do |week| %>
        <h1>
            <%= link_to week.week_name, instructor_week_titles_path(week)  %>
            <%=link_to  edit_instructor_program_name_week_path(program_name,week),  class: "edit-symbol" do %>
            <i class="far fa-edit" style="color:white"></i>
            <% end %>
            <%= link_to instructor_program_name_week_path(program_name,week), method: :delete, data: {confirm: 'Are you sure you want to delete this?'},class: 'delete-symbol' do %>
            <i class="far fa-trash-alt" style='color:white'></i>
            <% end %>
        </h1>
        <% end %>
        <br />
        <% end %>
        <div class="text-center">
            <%= link_to 'Build Week', new_instructor_program_name_week_path(@program_name), class: 'btn btn-success btn-large' %>
        </div>
        <hr />
    </div>
</div>
<br />
<div class="text-center">
    <%= link_to 'Homepage', root_path, class: 'btn btn-success btn-large' %>
</div>