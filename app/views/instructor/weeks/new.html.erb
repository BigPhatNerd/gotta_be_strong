<br />
<div class="booyah-box col-10 offset-1  text-center">
    <h1> Add Week Number</h1>
    <%= simple_form_for @week, url: instructor_weeks_path do |f| %>
    <div class="container ">
        <div class="row">
            <div class="col-8 offset-2">
                <%= f.input :week_name, placeholder: "ex Week 1 " %>
            </div>
        </div>
        <%= f.submit "Create", class: 'btn btn-success' %>
        <% end %>
    </div>
</div>