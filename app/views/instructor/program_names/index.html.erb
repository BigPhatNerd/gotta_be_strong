<div class="booyah-box col-10 offset-1">
    <h1 class="text-center">Programs Names Index Page</h1>
    <hr />
    <% @program_names.each do |program_name| %>
    <h1 class="text-center">
        <hr />
        <%=  link_to program_name.program_name, instructor_program_name_weeks_path(program_name) %>
        <%=link_to  edit_instructor_program_name_path(program_name),  class: "edit-symbol" do %>
        <i class="far fa-edit" style="color:white"></i>
        <% end %>
        <%= link_to instructor_program_name_path(program_name), method: :delete, data: {confirm: 'Are you sure you want to delete this?'},class: 'delete-symbol' do %>
        <i class="far fa-trash-alt" style='color:white'></i>
        <% end %>
    </h1>
    <div class="text-center">
        <%= link_to 'Build Week', new_instructor_program_name_week_path(program_name), class: 'btn btn-success btn-large' %>
    </div>
    <br />
    <% end %>
    <br />
    <br />
</div>
<br />
<br />
<div class="text-center">
    <%= link_to 'Homepage', root_path, class: 'yellow btn btn-whatever' %>
</div>