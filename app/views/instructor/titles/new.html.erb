<div class="booyah-box col-10 offset-1  text-center">
    <h1> Add a new Workout Day</h1>
    <%= simple_form_for @title, url: instructor_week_titles_path do |f| %>
    <div class="container ">
        <div class="row">
            <div class="col-8 offset-2">
                <%= f.input :title, placeholder: "ex Week1 Day1 " %>
            </div>
        </div>
        <br />
        <%= f.submit "Create", class: 'btn btn-success' %>
        <% end %>
    </div>
    <br />
</div>