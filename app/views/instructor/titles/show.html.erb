<div class="booyah-box col-10 offset-1">
    <h1 class="text-center">
        <%=@title.title%>
    </h1>
    <br />
    <%= link_to 'Preview As Student...', title_path(@title), class: 'btn btn-warning float-right' %>
    <br class="clr" />
    <div class="text-center">
        <%= link_to 'Add a new workout...', new_instructor_title_program_path(@title), class: 'btn btn-primary btn-lg' %>
    </div>
    <ul>
        <% @title.programs.each do |program| %>
        <li>
            <%= program.exercise %>
        </li>
        <% end %>
    </ul>
</div>