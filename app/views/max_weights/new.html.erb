<br />
<div class="booyah-box col-10 offset-1  text-center">
    <h1> Enter Max Weights</h1>
    <%= simple_form_for @max_weight, url: max_weights_path do |f| %>
    <br />
    <div class="container ">
        <div class="row">
            <div class="col-10 offset-1">
                <h3>
                    <%= f.input :max_bench, required: true, placeholder: "ex: 250 ", include_blank: false %>
                </h3>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-10 offset-1">
                <h3>
                    <%= f.input :max_squat, required: true, placeholder: "ex: 300" %>
                </h3>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col-10 offset-1">
                <h3>
                    <%= f.input :max_deadlift, required: true, placeholder: "ex: 400" %>
                </h3>
            </div>
        </div>
        <br />
        <%= f.submit "Submit Weights", class: 'btn btn-success' %>
        <% end %>
        <br />
    </div>
    <br />
</div>
